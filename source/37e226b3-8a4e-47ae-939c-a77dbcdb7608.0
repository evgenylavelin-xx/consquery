Процедура ВыборМенюДействие(Кнопка, ДополнительныеПараметры) Экспорт
	
	Если Кнопка = Неопределено Тогда
		Возврат;
	КонецЕсли;
	
	Если Кнопка.Значение = ""ОПрограмме"" Тогда
		ОткрытьФорму(""ВнешняяОбработка.КонсольЗапросов.Форма.уфИсторияИзменений"")
	ИначеЕсли Кнопка.Значение = ""СохранитьЗапросыВДругойФайл"" Тогда
		СохранитьФайлСЗапросом(Неопределено);
	ИначеЕсли Кнопка.Значение = ""НастройкиFTP"" Тогда
		НастройкиFTP();
	ИначеЕсли Кнопка.Значение = ""СохранитьНаFTP"" Тогда
		СохранитьНаFTP();
	ИначеЕсли Кнопка.Значение = ""ЗагрузитьСFTP"" Тогда
		ЗагрузитьСFTP();
	Иначе
		ЗапуститьПриложение(гНажатиеНаКнопкуВыборМенюИнформацияНаСервере(Кнопка.Значение, Истина));
	КонецЕсли;
		
КонецПроцедуры // ВыборМенюДействие()

