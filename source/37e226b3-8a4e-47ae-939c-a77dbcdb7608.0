#Область Меню

&НаСервереБезКонтекста
Функция ПолучитьМенюНаСервереСписком(ИмяМеню, ТипОбработка, СеансовыеДанные)
	
	лМенюСписком = ОбъектОбработки(ТипОбработка, СеансовыеДанные).гПолучитьМеню(ИмяМеню,Истина,Истина);
	
	Результат = Новый СписокЗначений;
	Для каждого ПунктМеню Из лМенюСписком Цикл
		
		Если ПунктМеню.Значение.Действие = Неопределено Тогда 
			Продолжить;
		КонецЕсли;
		
		Результат.Добавить(ПунктМеню.Значение.Имя, ПунктМеню.Значение.Название,,ПунктМеню.Значение.Картинка);
	КонецЦикла; 
	
	Возврат Результат;
	
КонецФункции // ПолучитьМенюНаСервереСписком()

&НаСервере
Процедура ЗаполнитьМенюПоДеревуНаСервере(Родитель, ДеревоМенюКнопки, СтопКран = 0)
	
	Если СтопКран > 1000 Тогда
		Возврат
	КонецЕсли;
	СтопКран = СтопКран + 1;
	
	лОкончаниеРазделителя = 1;
	Для каждого ТекСтрока Из ДеревоМенюКнопки.Строки Цикл
		
		Если лОкончаниеРазделителя = 1 Тогда 
			Если Родитель.ПодчиненныеЭлементы.Количество() > 0 Тогда 
				лГруппаКнопок = Элементы.Вставить(ТекСтрока.Имя + Формат(лОкончаниеРазделителя, ""ЧГ=0""), Тип(""ГруппаФормы""), Родитель, Родитель.ПодчиненныеЭлементы[0]);
			Иначе
				лГруппаКнопок = Элементы.Добавить(ТекСтрока.Имя + Формат(лОкончаниеРазделителя, ""ЧГ=0""), Тип(""ГруппаФормы""), Родитель);
			КонецЕсли;
			лГруппаКнопок.Вид = ВидГруппыФормы.ГруппаКнопок;
			лОкончаниеРазделителя = лОкончаниеРазделителя + 1;      
		КонецЕсли;

		Если Не ЗначениеЗаполнено(ТекСтрока.Действие) Тогда			
			Если ТекСтрока.Строки.Количество() > 0 Тогда
				лГруппаКнопок = Элементы.Добавить(ТекСтрока.Имя, Тип(""ГруппаФормы""), лГруппаКнопок);
				лГруппаКнопок.Вид = ВидГруппыФормы.ГруппаКнопок;
				лГруппаКнопок.Заголовок = ТекСтрока.Название;
				ЗаполнитьМенюПоДеревуНаСервере(лГруппаКнопок, ТекСтрока, СтопКран)
			Иначе
				лГруппаКнопок = Элементы.Добавить(ТекСтрока.Имя + Формат(лОкончаниеРазделителя, ""ЧГ=0""), Тип(""ГруппаФормы""), лГруппаКнопок);
				лГруппаКнопок.Вид = ВидГруппыФормы.Подменю;
				лОкончаниеРазделителя = лОкончаниеРазделителя + 1;
			КонецЕсли;	
		Иначе
			КомандаФормы  = Команды.Найти(ТекСтрока.Действие);
			
			Если КомандаФормы = Неопределено Тогда 
				КомандаФормы = Команды.Добавить(ТекСтрока.Действие);
				КомандаФормы.Действие = ТекСтрока.Действие;
			КонецЕсли;
			
			Кнопка            = Элементы.Добавить(ТекСтрока.Имя, Тип(""КнопкаФормы""), лГруппаКнопок);
			Кнопка.ИмяКоманды = ТекСтрока.Действие;
			Кнопка.Заголовок  = ТекСтрока.Название;
			
			Если ТекСтрока.СочетаниеКлавиш <> Неопределено Тогда
				Кнопка.СочетаниеКлавиш = ТекСтрока.СочетаниеКлавиш;
			КонецЕсли;
		КонецЕсли;
	КонецЦикла; 
	
КонецПроцедуры // ЗаполнитьМенюПоДеревуНаСервере()
 
&НаКлиенте
Процедура МенюИнформация(Команда)
	Оповещение = Новый ОписаниеОповещения(""ВыборМенюДействие"", Вычислить(ЭтотОбъектФорма)); 
	ПоказатьВыборИзМеню(Оповещение, ПолучитьМенюНаСервереСписком(""Информация"", ТипОбработка, СеансовыеДанные));
КонецПроцедуры

&НаКлиенте
