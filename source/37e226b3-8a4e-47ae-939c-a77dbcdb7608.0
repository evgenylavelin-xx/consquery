Функция ПолучитьСледующееУникальноеИмя(Знач ИсходноеИмя, Коллекция, ИмяПоля)
	
	Сч = 0;
	Пока НайтиЗначениеБезУчетаРегистра(ИсходноеИмя + ?(Сч = 0, """", Сч), Коллекция, ИмяПоля) <> Неопределено Цикл
		Сч = Сч + 1;
	КонецЦикла;

	Возврат ИсходноеИмя + ?(Сч = 0, """", Сч);

КонецФункции

